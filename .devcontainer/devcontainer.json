{
  "image": "ghcr.io/skyctl-space/devcontainer:25.05.0",
  "containerUser": "vscode",
  "customizations": {
    "vscode": {
      "extensions": [
        "rust-lang.rust-analyzer",
        "esbenp.prettier-vscode",
        "dbaeumer.vscode-eslint",
        "tauri-apps.tauri-vscode",
        "vue.volar",
        "vadimcn.vscode-lldb"
      ],
      "settings": {
        "rust-analyzer.cargo.runBuildScripts": true
      }
    }
  }
}
